{
  "name": "svelte-ts-tailwind-playwright-histoire",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "lint": "prettier . --check && eslint . --cache",
    "format": "prettier . --write && eslint . --cache --fix",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview --port 3000",
    "playwright": "playwright test",
    "playwright:ct": "playwright test -c playwright-ct.config.ts",
    "playwright:report": "playwright show-report",
    "histoire": "histoire dev"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "svelte-fa": "^4.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@histoire/plugin-svelte": "^0.17.17",
    "@playwright/experimental-ct-svelte": "^1.47.2",
    "@playwright/test": "^1.47.2",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@tsconfig/svelte": "^5.0.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-svelte": "^2.44.1",
    "globals": "^15.10.0",
    "histoire": "^0.17.17",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.6",
    "svelte-eslint-parser": "^0.41.1",
    "svelte-loader": "^3.2.3",
    "svelte-preprocess": "^6.0.3",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0",
    "vite": "^5.4.8"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  }
}
