{
  "name": "svelte-ts-tailwind-cypress-storybook",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "lint": "prettier . --check && eslint . --cache",
    "format": "prettier . --write && eslint . --cache --fix",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview --port 3000",
    "cypress": "start-server-and-test dev http://localhost:3000 cypress:open",
    "cypress:ci": "start-server-and-test preview http://localhost:3000 cypress:run",
    "cypress:open": "cypress open --e2e --browser electron",
    "cypress:run": "cypress run --spec 'cypress/e2e/*.cy.ts'",
    "storybook": "storybook dev -p 6006"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "svelte-fa": "^4.0.2"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^8.0.5",
    "@storybook/addon-essentials": "^8.0.5",
    "@storybook/addon-interactions": "^8.0.5",
    "@storybook/jest": "^0.2.3",
    "@storybook/svelte": "^8.0.5",
    "@storybook/svelte-vite": "^8.0.5",
    "@storybook/testing-library": "^0.2.2",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@tsconfig/svelte": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "autoprefixer": "^10.4.19",
    "cypress": "^13.7.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "start-server-and-test": "^2.0.3",
    "storybook": "^8.0.5",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.8",
    "svelte-eslint-parser": "^0.33.1",
    "svelte-loader": "^3.2.0",
    "svelte-preprocess": "^5.1.3",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.3",
    "vite": "^5.2.7"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  }
}
