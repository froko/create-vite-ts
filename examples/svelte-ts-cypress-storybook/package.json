{
  "name": "svelte-ts-cypress-storybook",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "format": "prettier . --write && eslint . --cache --fix",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview --port 3000",
    "cypress": "start-server-and-test dev http://localhost:3000 cypress:open",
    "cypress:ci": "npm run build && start-server-and-test preview http://localhost:3000 cypress:run",
    "cypress:open": "cypress open --e2e --browser electron",
    "cypress:run": "cypress run --spec 'cypress/e2e/*.cy.ts'",
    "storybook": "storybook dev -p 6006"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "fa-svelte": "^3.1.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^7.6.10",
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/jest": "^0.2.3",
    "@storybook/svelte": "^7.6.10",
    "@storybook/svelte-vite": "^7.6.10",
    "@storybook/testing-library": "^0.2.2",
    "@sveltejs/vite-plugin-svelte": "^2.4.5",
    "@tsconfig/svelte": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "cypress": "^13.6.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-svelte": "^2.32.4",
    "prettier": "^3.0.2",
    "prettier-plugin-svelte": "^3.0.3",
    "start-server-and-test": "^2.0.3",
    "storybook": "^7.6.10",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.0",
    "svelte-eslint-parser": "^0.32.2",
    "svelte-loader": "^3.1.9",
    "svelte-preprocess": "^5.0.4",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  }
}
