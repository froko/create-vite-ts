{
  "name": "@froko/create-vite-ts",
  "version": "7.1.0",
  "description": "A CLI to bootstrap new vite-based TypeScript projects",
  "keywords": [
    "cli",
    "create-vite-ts"
  ],
  "author": "Patrick Ineichen <github@frokonet.ch>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/froko/create-vite-ts.git"
  },
  "license": "MIT",
  "scripts": {
    "lint": "prettier . --check && eslint . --cache",
    "format": "prettier . --write && eslint . --cache --fix",
    "build": "node ./esbuild.mjs",
    "dev": "pnpm build && node dist/index.js",
    "playground": "pnpm build && node playground.mjs",
    "release": "semantic-release"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "bin": {
    "@froko/create-vite-ts": "dist/index.js",
    "create-vite-ts": "dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "next",
        "channel": "next",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md",
          "changelogTitle": "# Semantic Release Changelog"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "dist",
            "CHANGELOG.md",
            "package.json",
            "package-lock.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "dependencies": {
    "colorette": "^2.0.20",
    "ejs": "^3.1.10",
    "execa": "^8.0.1",
    "inquirer": "^9.2.20",
    "listr": "^0.14.3",
    "pkg-install": "^1.0.0",
    "ts-command-line-args": "^2.5.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.0",
    "@types/ejs": "^3.1.5",
    "@types/inquirer": "^9.0.7",
    "@types/listr": "^0.14.9",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "esbuild": "^0.20.2",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "nx": "^18.3.4",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "semantic-release": "^23.0.8",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@9.0.6+sha256.0624e30eff866cdeb363b15061bdb7fd9425b17bc1bb42c22f5f4efdea21f6b3"
}
